  <exceptionless apiKey="API_KEY_HERE"/>

  <clientDependency version="1" />

  <Escc.ClientDependencyFramework>
    <CssFiles>
      <add key="EastSussex1Space" value="https://www.eastsussex.gov.uk/escc.eastsussexgovuk/css/1space.css" />
