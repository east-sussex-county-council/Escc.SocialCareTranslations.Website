<?xml version="1.0"?>
<configuration>
  <configSections>
    <sectionGroup name="Escc.Net">
      <section name="Proxy" type="System.Configuration.NameValueSectionHandler, System, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"/>
    </sectionGroup>
    <sectionGroup name="EsccWebTeam.EastSussexGovUK">
      <section name="GeneralSettings" type="System.Configuration.NameValueSectionHandler, System, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"/>
      <section name="BreadcrumbTrail" type="System.Configuration.NameValueSectionHandler, System, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"/>
      <section name="RemoteMasterPage" type="System.Configuration.NameValueSectionHandler, System, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"/>
    </sectionGroup>
    <sectionGroup name="Escc.Web.Metadata">
      <section name="EgmsWebMetadata" type="Escc.Web.Metadata.EgmsWebMetadataSectionHandler,Escc.Web.Metadata, Version=1.0.0.0, Culture=neutral, PublicKeyToken=06fad7304560ae6f" requirePermission="false"/>
    </sectionGroup>
    <section name="clientDependency" type="ClientDependency.Core.Config.ClientDependencySection, ClientDependency.Core" requirePermission="false" />
    <sectionGroup name="Escc.ClientDependencyFramework">
      <section name="CssFiles" type="System.Configuration.NameValueSectionHandler, System, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
      <section name="MediaQueries" type="System.Configuration.NameValueSectionHandler, System, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
      <section name="ScriptFiles" type="System.Configuration.NameValueSectionHandler, System, Version=1.0.5000.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
    </sectionGroup>
    <section name="exceptionless" type="Exceptionless.Configuration.ExceptionlessSection, Exceptionless"/>
  </configSections>
  <exceptionless apiKey="API_KEY_HERE"/>
  <Escc.Web.Metadata>
    <EgmsWebMetadata errorMode="On" creator="Web Team, East Sussex County Council, County Hall, Lewes, BN7 1UE. https://www.eastsussex.gov.uk/contact-us/" lgilType="Providing information" openGraphType="article" copyrightUrl="https://new.eastsussex.gov.uk/about-this-site/copyright/" facebookAppId="169406409819518" hasTouchIcon="true" openSearchUrl="https://new.eastsussex.gov.uk/search/opensearch.xml" openSearchTitle="East Sussex County Council" siteName="East Sussex County Council" siteImageUrl="https://new.eastsussex.gov.uk/img/logo-for-facebook.png" titlePattern="{0} &amp;#8211; East Sussex County Council" twitterAccount="@eastsussexcc" windowsTileIconUrl="https://new.eastsussex.gov.uk/windows-tile.png" windowsTileColour="#ffffff" />
  </Escc.Web.Metadata>

  <clientDependency version="1">
    <!-- Full config documentation is here: https://github.com/Shazwazza/ClientDependency/wiki/Configuration -->
    <mvc defaultRenderer="PolyfillMediaQueryRenderer">
      <renderers>
        <add name="PolyfillMediaQueryRenderer" type="Escc.ClientDependencyFramework.PolyfillMediaQueryRenderer, Escc.ClientDependencyFramework" />
      </renderers>
    </mvc>
  </clientDependency>
  
  <Escc.ClientDependencyFramework>
    <CssFiles>
      <add key="EastSussex1Space" value="/escc.eastsussexgovuk/css/1space.css" />
      <add key="ContentSmall" value="/escc.eastsussexgovuk/css/content-small.css" />
      <add key="ContentMedium" value="/escc.eastsussexgovuk/css/content-medium.css" />
      <add key="ContentLarge" value="/escc.eastsussexgovuk/css/content-large.css" />
    </CssFiles>
    <ScriptFiles>
      <add key="HandlerPlaceholder" value="javascript" />
      <add key="EastSussex1Space" value="/escc.eastsussexgovuk/js/1space.js" />
    </ScriptFiles>
    <MediaQueries>
      <add key="Medium" value="only screen and (min-width: 474px)" />
      <add key="Large" value="only screen and (min-width: 802px)" />
    </MediaQueries>
  </Escc.ClientDependencyFramework>
  <system.web>
    <compilation targetFramework="4.5.2">
      <assemblies>
        <add assembly="System.Net.Http, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
      </assemblies>
    </compilation>
    <httpRuntime targetFramework="4.5"/>
    <pages masterPageFile="~/masterpages/desktop.master">
      <controls>
        <add tagPrefix="EastSussexGovUK" namespace="EsccWebTeam.EastSussexGovUK" assembly="EsccWebTeam.EastSussexGovUK, Version=1.0.0.0, Culture=neutral, PublicKeyToken=06fad7304560ae6f" />
        <add tagPrefix="EastSussexGovUK" namespace="EsccWebTeam.EastSussexGovUK.MasterPages.Controls" assembly="EsccWebTeam.EastSussexGovUK, Version=1.0.0.0, Culture=neutral, PublicKeyToken=06fad7304560ae6f" />
        <add tagPrefix="EastSussexGovUK" tagName="Share" src="~/masterpages/controls/share.ascx" />
        <add tagPrefix="ClientDependency" namespace="Escc.ClientDependencyFramework.WebForms" assembly="Escc.ClientDependencyFramework.WebForms" />
        <add tagPrefix="Metadata" namespace="Escc.Web.Metadata" assembly="Escc.Web.Metadata" />
      </controls>
      <namespaces>
        <add namespace="ClientDependency.Core" />
        <add namespace="ClientDependency.Core.Mvc" />
      </namespaces>
    </pages>
  </system.web>
  <!--<Escc.Net>
    <Proxy>
      <add key="Server" value=""/>
      <add key="User" value=""/>
      <add key="Domain" value=""/>
      <add key="Password" value=""/>
    </Proxy>
  </Escc.Net>-->
  <EsccWebTeam.EastSussexGovUK>
    <GeneralSettings>
      <add key="DesktopMvcLayout" value="~/views/eastsussexgovuk/desktop.cshtml" />
      <add key="PlainMvcLayout" value="~/views/eastsussexgovuk/plain.cshtml" />
      <add key="FullScreenMvcLayout" value="~/views/eastsussexgovuk/fullscreen.cshtml" />
      <add key="MasterPageParameterName" value="template"/>
    </GeneralSettings>
    <BreadcrumbTrail>
      <add key="Home" value="/"/>
      <add key="Adult social care and health" value="/socialcare/"/>
      <add key="About us" value="/socialcare/about-us/" />
    </BreadcrumbTrail>
    <RemoteMasterPage>
      <add key="CacheMinutes" value="60"/>
      <add key="MasterPageControlUrl" value="https://www.eastsussex.gov.uk/masterpages/remote/control.aspx?control={0}"/>
    </RemoteMasterPage>
  </EsccWebTeam.EastSussexGovUK>
  <system.webServer>
    <modules>
      <add name="MasterPageModule" type="EsccWebTeam.EastSussexGovUK.MasterPages.MasterPageModule, EsccWebTeam.EastSussexGovUK"/>
      <add name="ExceptionlessModule" type="Exceptionless.Mvc.ExceptionlessModule, Exceptionless.Mvc" />
      <remove name="ClientDependencyModule" />
      <add name="ClientDependencyModule" type="ClientDependency.Core.Module.ClientDependencyModule, ClientDependency.Core" />
      <remove name="ContentSecurityPolicy"/>
      <add name="ContentSecurityPolicy" type="Escc.Web.ContentSecurityPolicyModule" />
    </modules>
    <handlers>
      <remove name="DependencyHandler" />
      <add name="DependencyHandler" preCondition="integratedMode" verb="GET" path="DependencyHandler.axd" type="ClientDependency.Core.CompositeFiles.CompositeDependencyHandler, ClientDependency.Core " />
    </handlers>
  </system.webServer>

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Helpers" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.WebPages" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Mvc" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="0.0.0.0-5.2.3.0" newVersion="5.2.3.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>