      </assemblies>
    </compilation>
    <httpRuntime targetFramework="4.7.2" enableVersionHeader="false" />
    <pages>
      <namespaces>
        <add namespace="ClientDependency.Core" />
        <add namespace="ClientDependency.Core.Mvc" />
      </namespaces>
    </pages>
  </system.web>
